{
	"name": "tree",
	"displayName": "tree",
	"description": "a tool of tree command utilized vscode gui functions",
	"version": "0.1.0",
	"license": "MIT",
	"publisher": "akiega",
	"engines": {
		"vscode": "^1.48.0"
	},
	"categories": [
		"Other"
	],
	"repository": {
		"url": "https://github.com/AkiEga/vscode-extension-tree"
	},
	"activationEvents": [
		"onView:fileTree"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "tree.cmd",
				"title": "show tree cmd results from file tree view"
			}
		],
		"menus": {
			"view/item/context": [
				{
					"command": "tree.cmd",
					"when": "view == fileTree",
					"group": "tree"
				}
			]
		},
		"views": {
			"explorer": [
				{
					"id": "fileTree",
					"name": "File Tree"
				}
			]
		}
	},
	"scripts": {
		"vscode:publish": "vsce publish minor -p $(cat ./PersonalAccessToken.txt) --yarn",
		"vscode:prepublish": "yarn run compile",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "yarn run compile && yarn run lint",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.3",
		"@types/marked": "^1.1.0",
		"@types/mocha": "^8.0.0",
		"@types/node": "^14.0.27",
		"@types/vscode": "^1.48.0",
		"@typescript-eslint/eslint-plugin": "^3.8.0",
		"@typescript-eslint/parser": "^3.8.0",
		"eslint": "^7.6.0",
		"glob": "^7.1.6",
		"mocha": "^8.0.1",
		"typescript": "^3.8.3",
		"vscode-test": "^1.4.0"
	},
	"dependencies": {
		"marked": "^1.2.0"
	}
}
