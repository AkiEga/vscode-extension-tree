{
	"name": "tree",
	"displayName": "tree",
	"description": "a tool of tree command utilized vscode gui functions",
	"version": "0.6.0",
	"license": "MIT",
	"publisher": "akiega",
	"engines": {
		"vscode": "^1.48.0"
	},
	"categories": [
		"Other"
	],
	"repository": {
		"url": "https://github.com/AkiEga/vscode-extension-tree"
	},
	"activationEvents": [
		"*"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "tree.cmd",
				"title": "show tree cmd results from file tree view"
			},
			{
				"command": "tree.refreshEntry",
				"title": "Refresh",
				"icon": {
					"light": "resources/light/refresh.svg",
					"dark": "resources/dark/refresh.svg"
				}
			}
		],
		"menus": {
			"view/item/context": [
				{
					"command": "tree.cmd",
					"when": "view == fileTree",
					"group": "tree"
				}
			],
			"view/title": [
				{
					"command": "tree.refreshEntry",
					"when": "view == fileTree",
					"group": "navigation"
				}
			]
		},
		"views": {
			"explorer": [
				{
					"id": "fileTree",
					"name": "File Tree",
					"visibility": "collapsed"
				}
			]
		}
	},
	"scripts": {
		"vscode:publish": "vsce publish minor -p $(cat ./PersonalAccessToken.txt) --yarn",
		"vscode:prepublish": "webpack --mode production",
		"compile": "webpack --mode development",
		"watch": "webpack --mode development --watch",
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "yarn compile && node ./node_modules/vscode/bin/test"
	},
	"devDependencies": {
		"@types/glob": "^7.1.3",
		"@types/marked": "^1.1.0",
		"@types/mocha": "^8.0.0",
		"@types/node": "^14.0.27",
		"@types/vscode": "^1.48.0",
		"@typescript-eslint/eslint-plugin": "^3.8.0",
		"@typescript-eslint/parser": "^3.8.0",
		"eslint": "^7.6.0",
		"glob": "^7.1.6",
		"mocha": "^8.0.1",
		"ts-loader": "^8.0.13",
		"typescript": "^3.8.3",
		"vscode-test": "^1.4.0",
		"webpack": "^5.11.1",
		"webpack-cli": "^4.3.1"
	},
	"dependencies": {
		"marked": "^1.2.0"
	}
}
