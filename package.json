{
	"name": "tree",
	"displayName": "tree",
	"description": "a tool of tree command utilized vscode gui functions",
	"version": "0.18.0",
	"license": "Beerware(SEE LICENSE IN [LICENSE.md](./LICENSE.md))",
	"publisher": "akiega",
	"engines": {
		"vscode": "^1.85.0"
	},
	"categories": [
		"Other"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/AkiEga/vscode-extension-tree.git"
	},
	"bugs": {
		"url": "https://github.com/AkiEga/vscode-extension-tree/issues"
	},
	"homepage": "https://github.com/AkiEga/vscode-extension-tree#readme",
	"activationEvents": [],
	"main": "./dist/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "tree.cmd",
				"title": "show tree cmd results from file tree view"
			},
			{
				"command": "tree.refreshEntry",
				"title": "Refresh",
				"icon": {
					"light": "resources/light/refresh.svg",
					"dark": "resources/dark/refresh.svg"
				}
			}
		],
		"menus": {
			"view/item/context": [
				{
					"command": "tree.cmd",
					"when": "view == fileTree",
					"group": "tree"
				}
			],
			"view/title": [
				{
					"command": "tree.refreshEntry",
					"when": "view == fileTree",
					"group": "navigation"
				}
			]
		},
		"views": {
			"explorer": [
				{
					"icon": "resources/tree-icon.drawio.svg",
					"id": "fileTree",
					"name": "File Tree",
					"visibility": "collapsed"
				}
			]
		},
		"configuration": {
			"title": "tree view",
			"properties": {
				"tree.view-type": {
					"type": "string",
					"enum": [
						"TextEditor",
						"WebViewPanel"
					],
					"default": "TextEditor",
					"description": "Tree view type. Please select TextEditor or WebViewPanel.\n TextEditor is a markdown file view mode. \n  WebViewPanel is a un-editable panel view mode."
				}
			}
		}
	},
	"scripts": {
		"prepublishOnly": "npm run package",
		"vscode:publish": "vsce publish minor",
		"compile": "webpack --mode development --devtool inline-source-map",
		"watch": "webpack --watch --mode development",
		"package": "webpack --mode production --devtool hidden-source-map",
		"compile-tests": "tsc -p . --outDir out",
		"watch-tests": "tsc -p . -w --outDir out",
		"pretest": "npm run compile-tests && npm run compile",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^8.1.0",
		"@types/mocha": "^10.0.1",
		"@types/node": "20.2.5",
		"@types/vscode": "^1.85.0",
		"@typescript-eslint/eslint-plugin": "^5.59.8",
		"@typescript-eslint/parser": "^5.59.8",
		"@vscode/test-electron": "^2.3.2",
		"eslint": "^8.41.0",
		"glob": "^8.1.0",
		"mocha": "^10.2.0",
		"ts-loader": "^9.4.3",
		"typescript": "^5.1.3",
		"webpack": "^5.85.0",
		"webpack-cli": "^5.1.1"
	}
}
